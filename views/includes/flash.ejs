<% if (success && success.length) { %>
      <div class="alert alert-success col-8 col-lg-4 col-md-4 col-sm-4 offset-3 mt-2 alert-dismissible fade show" role="alert">
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>
